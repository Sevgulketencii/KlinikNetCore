@model List<Hakkinda>


<div class="page-section">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-8 wow fadeInUp">
                @foreach (var item in Model)
                {
                    <h1 class="text-center mb-3"><strong>@item.HakkindaBaslik</strong></h1>
                    <br />
                    <div class="text-lg">
                        <p>@item.HakkindaAciklama</p>
                    </div>
                }

            </div>

        </div>
    </div>
</div>